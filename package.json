{
  "name": "admin2hats",
  "version": "3.0.0-dev",
  "private": true,
  "dependencies": {
    "@bit/sidney2hats.2hats.global.common-constants": "file:./bit/global/common-constants",
    "@date-io/moment": "^1.0.1",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/lab": "^4.0.0-alpha.28",
    "@material-ui/styles": "^4.5.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "algoliasearch": "^3.30.0",
    "classnames": "^2.2.6",
    "echarts": "^4.2.0-rc.2",
    "echarts-for-react": "^2.0.15-beta.0",
    "firebase": "^5.4.2",
    "formik": "^1.4.1",
    "fs": "^0.0.1-security",
    "git-rev-sync": "^1.12.0",
    "lodash": "^4.17.11",
    "material-ui-pickers": "^2.1.1",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "pica": "^5.0.0",
    "query-string": "5.1.1",
    "ramda": "^0.26.0",
    "randomcolor": "^0.5.3",
    "react": "^16.8.1",
    "react-beautiful-dnd": "^10.0.4",
    "react-color": "^2.14.1",
    "react-cropper": "^1.2.0",
    "react-dom": "^16.8.1",
    "react-dropzone": "^4.2.11",
    "react-email-editor": "^1.0.0",
    "react-grid-layout": "^0.16.6",
    "react-infinite-scroller": "^1.2.2",
    "react-places-autocomplete": "^7.2.1",
    "react-quill": "^1.3.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.2.0",
    "react-select": "^2.1.2",
    "typescript": "^3.6.3",
    "yup": "^0.26.6"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/App.*",
    "start": "PORT=3000 react-scripts start",
    "build": "node scripts/generate-build-id.js && react-scripts build",
    "test": "node scripts/test.js --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@material-ui/codemod": "^4.5.0",
    "husky": "^1.2.1",
    "prettier": "1.15.3",
    "pretty-quick": "^1.8.0",
    "source-map-explorer": "^1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
